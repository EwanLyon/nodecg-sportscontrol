{
	"name": "nodecg-sportscontrol",
	"version": "0.0.0",
	"description": "",
	"homepage": "",
	"author": {
		"name": "Ewan"
	},
	"files": [
		"dashboard",
		"graphics",
		"extension.js",
		"extension"
	],
	"keywords": [
		"",
		"nodecg-bundle"
	],
	"nodecg": {
		"compatibleRange": "^1.8.1",
		"dashboardPanels": [
			{
				"name": "teampresetcreator",
				"title": "Team/Player Creator",
				"width": 4,
				"file": "team-preset-creator.html",
				"workspace": "Setup"
			},
			{
				"name": "schedule",
				"title": "Schedule",
				"width": 5,
				"file": "schedule.html",
				"workspace": "Setup"
			},
			{
				"name": "tournament",
				"title": "Tournament",
				"file": "tournament.html",
				"fullbleed": true
			}
		],
		"assetCategories": [
			{
				"name": "teamimages",
				"title": "Team Logo",
				"allowedTypes": [
					"jpg",
					"jpeg",
					"gif",
					"png",
					"svg"
				]
			},
			{
				"name": "playerIcons",
				"title": "Player Icons",
				"allowedTypes": [
					"jpg",
					"jpeg",
					"gif",
					"png",
					"svg"
				]
			},
			{
				"name": "tournamentLogos",
				"title": "Tournament Logos",
				"allowedTypes": [
					"jpg",
					"jpeg",
					"gif",
					"png",
					"svg"
				]
			},
			{
				"name": "teamPreset",
				"title": "Team Preset",
				"allowedTypes": [
					"json"
				]
			}
		]
	},
	"scripts": {
		"build": "webpack",
		"dev:tsc": "yarn test:types -w --preserveWatchOutput",
		"dev:webpack": "cross-env WEBPACK_WATCH=true webpack",
		"start": "node ../..",
		"test:prettier": "prettier \"**/*.{ts,tsx,json,html,yml,css,scss,md}\" --check",
		"test:lint": "eslint --ext .ts ."
	},
	"repository": "https://github.com/ewanlyon/nodecg-sportscontrol.git",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/plugin-proposal-class-properties": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"@emotion/react": "^11.5.0",
		"@emotion/styled": "^11.3.0",
		"@mui/material": "^5.1.0",
		"@types/lodash": "^4.14.176",
		"@types/mini-css-extract-plugin": "^2.4.0",
		"@types/react-beautiful-dnd": "^13.1.2",
		"@types/react-dom": "^17.0.11",
		"@types/styled-components": "^5.1.15",
		"@types/uuid": "^8.3.1",
		"@types/webpack-bundle-analyzer": "^4.4.1",
		"@types/webpack-node-externals": "^2.5.3",
		"clean-webpack-plugin": "^4.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"eslint": "^7.32.0",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"file-loader": "^6.2.0",
		"globby": "^11.0.4",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.4.4",
		"prettier": "^2.4.1",
		"react": "^17.0.2",
		"react-beautiful-dnd": "^13.1.0",
		"react-dom": "^17.0.2",
		"react-twemoji": "^0.3.0",
		"styled-components": "^5.3.3",
		"trash-cli": "^4.0.0",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.4",
		"use-nodecg": "^0.4.0",
		"webpack": "^5.62.1",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.9.1",
		"webpack-merge": "^5.8.0",
		"webpack-node-externals": "^3.0.0",
		"webpackbar": "^5.0.2"
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"uuid": "^8.3.2"
	}
}
